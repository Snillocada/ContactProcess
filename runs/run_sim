#!/bin/bash

chmod o+r stensils/*

LAMBDAS=('0.3' '0.4' '0.5' '0.6' '0.7' '0.8' '0.9' '1.0')
FILES=stensils/*
for file in $FILES
do
	current_file=$(echo $(basename $file))   
	number=$(echo $current_file | tr -dc '0-9')  
	for lambda in "${LAMBDAS[@]}"
	do
		current_file=$(echo $(basename $file))
		number=$(echo $current_file | tr -dc '0-9')
		./CPLinux $number 10000 3 $lambda p &
	done
	wait
	echo $number
done
