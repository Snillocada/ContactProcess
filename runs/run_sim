#!/bin/bash

chmod o+r stensils/*

LAMBDAS=('1.1' '1.2' '1.3' '1.4' '1.5' '1.6' '1.7' '1.8')
FILES=stensils/*
for file in $FILES
do
	current_file=$(echo $(basename $file))   
	number=$(echo $current_file | tr -dc '0-9')  
	for lambda in "${LAMBDAS[@]}"
	do
		current_file=$(echo $(basename $file))
		number=$(echo $current_file | tr -dc '0-9')
		./CPLinux $number 1000 1 $lambda p &
	done
	wait
	echo $number
done
