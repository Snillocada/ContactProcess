#!/bin/bash

chmod o+r stensils/*

LAMBDAS=('0.5' '0.51' '0.52' '0.53' '0.54' '0.55' '0.56' '0.57')
FILES=stensils/*
for file in $FILES
do
	current_file=$(echo $(basename $file))   
	number=$(echo $current_file | tr -dc '0-9')  
	for lambda in "${LAMBDAS[@]}"
	do
		current_file=$(echo $(basename $file))
		number=$(echo $current_file | tr -dc '0-9')
		./CPLinux $number 35000 3 $lambda p &
	done
	wait
	echo $number
done
